{
    "ui_font_size": 16,
    "buffer_font_size": 16,
    "buffer_font_family": "MesloLGM Nerd Font",
    "theme": {
      "mode": "system",
      "light": "One Light",
      "dark": "Ayu Dark"
    },
    "experimental.theme_overrides": {
      "terminal": {
        "font_family": "MesloLGM Nerd Font",
        "foreground": "#c8c8c8"
      },
      "terminal.ansi.black": "#222222",
      "terminal.ansi.blue": "#0599d0",
      "terminal.ansi.cyan": "#00a6b2",
      "terminal.ansi.green": "#5c9e4f",
      "terminal.ansi.magenta": "#66018d",
      "terminal.ansi.red": "#e45124",
      "terminal.ansi.white": "#ffffff",
      "terminal.ansi.yellow": "#f9ae02",
      "terminal.ansi.bright_black": "#222222",
      "terminal.ansi.bright_blue": "#0599d0",
      "terminal.ansi.bright_cyan": "#00a6b2",
      "terminal.ansi.bright_green": "#5c9e4f",
      "terminal.ansi.bright_magenta": "#66018d",
      "terminal.ansi.bright_red": "#e45124",
      "terminal.ansi.bright_white": "#ffffff",
      "terminal.ansi.bright_yellow": "#f9ae02",
      "terminal.ansi.dim_black": "#222222",
      "terminal.ansi.dim_blue": "#0599d0",
      "terminal.ansi.dim_cyan": "#00a6b2",
      "terminal.ansi.dim_green": "#5c9e4f",
      "terminal.ansi.dim_magenta": "#66018d",
      "terminal.ansi.dim_red": "#e45124",
      "terminal.ansi.dim_white": "#ffffff",
      "terminal.ansi.dim_yellow": "#f9ae02",
      "terminal.foreground": "#ededed",
      "terminal.bright_foreground": "#ededed",
      "players": [
        {
          "cursor": "#1798e0"
        }
      ]
    },
    "terminal": {
      "dock": "bottom",
      "working_directory": "current_project_directory",
      "blinking": "terminal_controlled",
      "alternate_scroll": "off",
      "option_as_meta": false,
      "copy_on_select": false,
      "button": true,
      "env": {},
      "line_height": "comfortable",
      "toolbar": {
        "title": true
      },
      "font_family": "MesloLGM Nerd Font"
    }
  }
  